{
  "name": "jsdoc-comment-toggler",
  "version": "1.10.2",
  "preview": true,
  "displayName": "jsdoc comment toggler",
  "description": "toggle JSDoc comment on line or selection",
  "icon": "images/jsdoc-icon.png",
  "galleryBanner": {
    "color": "#34223A",
    "theme": "dark"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "hardesty",
      "hardesty/typescript"
    ],
    "env": {
      "mocha": true,
      "webextensions": true
    },
    "ignorePatterns": "*.js",
    "rules": {
      "unicorn/import-style": "off"
    }
  },
  "prettier": {
    "jsdocCapitalizeDescription": false,
    "semi": false
  },
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jsdoc-comment-toggler.toggle"
  ],
  "main": "./out/src/extension.js",
  "publisher": "zachhardesty",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachhardesty7/jsdoc-comment-toggler"
  },
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "jsdoc-comment-toggler.toggle",
        "title": "Toggle JSDoc Comment"
      }
    ]
  },
  "keybindings": [
    {
      "key": "ctrl+r ctrl+/",
      "command": "jsdoc-comment-toggler.toggle",
      "when": "!isMac && editorTextFocus && !editorReadonly"
    },
    {
      "key": "cmd+r cmd+/",
      "command": "jsdoc-comment-toggler.toggle",
      "when": "isMac && editorTextFocus && !editorReadonly"
    }
  ],
  "scripts": {
    "clean": "rm -rf ./out",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "_pack": "vsce package",
    "_upload": "vsce publish",
    "pub": "npm-run-all clean compile _pack _upload",
    "_run-test": "node -r dotenv/config --async-stack-traces ./out/test/runTest.js",
    "test": "npm-run-all compile _run-test"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.0",
    "@types/vscode": "^1.61.0",
    "@vscode/vsce": "^2.17.0",
    "ansi-colors": "^4.1.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-hardesty": "^6.1.1",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "source-map-support": "^0.5.21",
    "typescript": "4.9.x",
    "vscode-test": "^1.6.1"
  }
}
